<?php
/**
 * Implements hook_theme().
 */
function moliek_theme($existing, $type, $theme, $path) {
  return [
    // Name of the theme hook. This is used in the controller to trigger the hook.
    'moliek_theme_hook' => [
      'variables' => [
        'text' => NULL,
        'form' => NULL,
      ],
    ],
  ];
}

